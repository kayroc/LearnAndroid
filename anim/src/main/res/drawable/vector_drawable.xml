<?xml version="1.0" encoding="utf-8"?>
<!--
    vector_drawable.xml    位于 res/drawable 目录

    vector：根标签
    android:name：对需要进行动画的 path 或 group 命名
    -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="1000"
    android:viewportHeight="1000">

    <group
        android:name="rotation"
        android:pivotX="500.0"
        android:pivotY="500.0"
        android:rotation="0.0">

        <path
            android:name="vector"
            android:fillColor="#00ffff"
            android:pathData="M67,750 L500,0 500,0 933,750 67,750
            M146,853 A 0.1,500 0 0 1 146,148
            M146,148 A 500,0.1 0 0 1 851,148
            M851,148 A 0.1,500 0 0 1 851,853
            M851,853 A 500,0.1 0 0 1 146,853" />
    </group>
</vector>