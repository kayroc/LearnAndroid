<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kayroc.android.learn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearnAndroid">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".file.FileActivity"
            android:label="文件存储"/>

        <activity android:name=".sp.SharedPreferencesActivity"
            android:label="SharedPreferences 存储"/>

        <activity android:name=".sql.SQLiteActivity"
            android:label="SQLite 存储"/>
    </application>

</manifest>